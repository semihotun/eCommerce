<script type="text/javascript">
    $(document).ready(function () {
                var searchboxID = "#txtSearch";
        $('#srcButton').click(function () {
            $(searchboxID).keyup();
                });
        $(searchboxID).keyup(function (event) {
            if (event.keyCode != 13 && event.keyCode != 37 && event.keyCode != 39) {
            if ($.trim($(searchboxID).val()).length >= 2) {
                delay(function () {
                        $.ajax({
                    url : '/Home/Search?searchKey=' + $(searchboxID).val(),
                            type: "POST",
                            success: function (result) {
                                $("#searchDiv").hide();
                                $("#searchDiv").html(result);
                                $("#searchDiv").show();
                                $("#searchDiv").css('z-index', '1');
                        }
                    });
                }, 100);
            } else {
                    $("#searchDiv").hide();
            }
        }
        });
    });
    $("body:not(#searchDiv):not(input#txtSearch)").click(function (event) {
            $("#searchDiv").hide();
    });
    var delay = (function () {
        var timer = 0;
        return function (callback, ms) {
            clearTimeout(timer);
            timer = setTimeout(callback, ms);
        };
    })();
    $(function () {
        $("#txtSearch").click(function () {
            if ($("#txtSearch").val() == "Detaylı ara?") {
                $("#txtSearch").val("");
            }
        });
        $("#txtSearch").blur(function () {
            if ($("#txtSearch").val().trim() == "") {
                $("#txtSearch").val("Detaylı ara?");
            }
        });
    });
    $(window).load(function () {
        $('.loadingbody').delay(200).fadeOut('slow', function () { });
    });
</script>
