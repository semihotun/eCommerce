<body ng-app="myApp">
    @if (@TempData["notification"] != null)
    {
        <script>
    @Html.Raw(@TempData["notification"]);
        </script>
    }
    <!-- HEADER -->
    <header>
        <!-- top Header -->
        <div id="top-header">
            <div class="container-fluid home-container" >
                <div class="pull-left">
                    <span>E-shop'a Hoşgeldiniz</span>
                </div>
                <div class="pull-right">
                    <ul class="header-top-links">
                        <li><a href="#">Mağazalarımız</a></li>
                        <li class="dropdown default-dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">Türkçe <i class="fa fa-caret-down"></i></a>
                            <ul class="custom-menu">
                                <li><a href="#">English (ENG)</a></li>
                                <li><a href="#">Russian (Ru)</a></li>
                                <li><a href="#">French (FR)</a></li>
                                <li><a href="#">Spanish (Es)</a></li>
                            </ul>
                        </li>
                        <li class="dropdown default-dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">TL <i class="fa fa-caret-down"></i></a>
                            <ul class="custom-menu">
                                <li><a href="#">USD ($)</a></li>
                                <li><a href="#">EUR (€)</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- /top Header -->
        <!-- header -->
        <div id="header">
            <div class="container-fluid home-container" >
                <div class="pull-left">
                    <!-- Mobile nav toggle-->
                    <li class="nav-toggle">
                        <button class="nav-toggle-btn main-btn icon-btn"><i class="fa fa-bars"></i></button>
                    </li>
                    <!-- / Mobile nav toggle -->
                    <!-- Logo -->
                    <div class="header-logo">
                        <a class="logo" href="~/Home/">
                            <img src="~/Content/Themes/img/logo.png" />
                        </a>
                    </div>
                    <!-- /Logo -->
                </div>
                <!-- Search -->
                <div class="header-search">
                    <form id="tfnewsearch">
                        <input type="text" id="txtSearch" class="tftextinput2 input search-input" name="q" size="21" maxlength="120" value="Detaylı ara?">
                        <button class="search-btn" id="srcButton"><i class="fa fa-search"></i></button>
                        <div class="divResult" id="searchDiv"></div>
                    </form>
                </div>
                <!-- /Search -->
                <div class="headermobil-search">
                    <form id="tfnewsearch">
                        <input type="text" id="txtSearch" class="tftextinput2 input search-input" name="q" size="21" maxlength="120" value="Detaylı ara?">
                        <button class="search-btn" id="srcButton"><i class="fa fa-search"></i></button>
                        <div class="divResult" id="searchDiv"></div>
                    </form>
                </div>
                <div class="pull-right">
                    <ul class="header-btns">
                        <!-- Account -->
                        <li class="header-account dropdown default-dropdown">
                            @if (User.Identity.IsAuthenticated)
                            {
                                <div class="dropdown-toggle" role="button" data-toggle="dropdown" aria-expanded="true">
                                    <div class="header-btns-icon">
                                        <i class="fa fa-user-o"> </i>
                                    </div>
                                    <strong class="text-uppercase">Hesabım &nbsp;<i class="fa fa-caret-down"></i></strong>
                                </div>
                                <ul class="custom-menu">
                                    @await Html.PartialAsync("~/Views/Manage/_ManageNav.cshtml")
                                    <li class="nav-item">
                                        <a href="/Account/Logout" class="  text-dark">Çıkış Yap</a>
                                    </li>
                                </ul>
                            }
                            else
                            {
                                <div class="dropdown-toggle" role="button" data-toggle="dropdown" aria-expanded="true">
                                    <div class="header-btns-icon">
                                        <i class="fa fa-user-o"> </i>
                                    </div>
                                </div>
                                <span style="padding-top:11px;display:block;font-weight:100;">
                                    <a href="~/Account/Login" class="text-uppercase">Giriş </a>
                                    <span> / </span>
                                    <a href="~/Account/Register" class="text-uppercase">Kayıt</a>
                                </span>
                            }
                        </li>
                        <!-- /Account -->
                        <!-- Cart -->
                        <li class="header-cart dropdown default-dropdown">
                            <a class="dropdown-toggle" aria-expanded="true" href="/Home/Checkout">
                                <div class="header-btns-icon">
                                    <i class="fa fa-shopping-cart"></i>
                                    <span class="qty">
                                        @Html.Partial("../Home/Basket")
                                    </span>
                                </div>
                                <strong class="text-uppercase" style="padding-top: 11px;
                             display: block;">Sepetim</strong>
                                <br>
                                @*<span>35.20$</span>*@
                            </a>
                            @*<div class="custom-menu">
                            <div id="shopping-cart">
                                <div class="shopping-cart-list">
                                    <div class="product product-widget">
                                        <div class="product-thumb">
                                            <img src="./img/thumb-product01.jpg" alt="">
                                        </div>
                                        <div class="product-body">
                                            <h3 class="product-price">$32.50 <span class="qty">x3</span></h3>
                                            <h2 class="product-name"><a href="#">Product Name Goes Here</a></h2>
                                        </div>
                                        <button class="cancel-btn"><i class="fa fa-trash"></i></button>
                                    </div>
                                    <div class="product product-widget">
                                        <div class="product-thumb">
                                            <img src="./img/thumb-product01.jpg" alt="">
                                        </div>
                                        <div class="product-body">
                                            <h3 class="product-price">$32.50 <span class="qty">x3</span></h3>
                                            <h2 class="product-name"><a href="#">Product Name Goes Here</a></h2>
                                        </div>
                                        <button class="cancel-btn"><i class="fa fa-trash"></i></button>
                                    </div>
                                </div>
                                <div class="shopping-cart-btns">
                                    <button class="main-btn">View Cart</button>
                                    <button class="primary-btn">Sepetim ><i class="fa fa-arrow-circle-right"></i></button>
                                </div>
                            </div>
                        </div>*@
                        </li>
                        <!-- /Cart -->
                    </ul>
                </div>
            </div>
            <!-- header -->
        </div>
        <!-- container -->
    </header>
    <!-- /HEADER -->
    <!-- NAVIGATION -->
    <!-- NAVIGATION -->
    @await Component.InvokeAsync("CategoryMenu")
    <!-- /NAVIGATION -->
    <!-- /NAVIGATION -->
